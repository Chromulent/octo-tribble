Cleanup
source <(curl -L https://raw.githubusercontent.com/Chromulent/octo-tribble/main/Automation/Download/Underactive%20Functions/Cleanup/_Cleanup.sh)






dir1="/mnt/octo/sets/tv/Judge Judy"

dir2="/media/bw/Xi/sets/tv/Judge Judy"

check_loop () {
	clear

	echo "${dir1}"
	ls "${dir1}"
	sleep 1s
	echo "${dir2}"
	ls "${dir2}"
}

while true; do check_loop; sleep 60; done




